<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<sDEFAULT>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:clubdirectory/Resources/Private/Language/FlexForms.xlf:sheetGeneral</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<settings.showGlossar>
						<TCEforms>
							<label>LLL:EXT:clubdirectory/Resources/Private/Language/FlexForms.xlf:showGlossar</label>
							<config>
								<type>check</type>
								<renderType>checkboxToggle</renderType>
								<default>1</default>
							</config>
						</TCEforms>
					</settings.showGlossar>
					<settings.showAllLink>
						<TCEforms>
							<label>LLL:EXT:clubdirectory/Resources/Private/Language/FlexForms.xlf:showAllLink</label>
							<config>
								<type>check</type>
								<renderType>checkboxToggle</renderType>
								<default>1</default>
							</config>
						</TCEforms>
					</settings.showAllLink>
					<settings.category>
						<TCEforms>
							<label>LLL:EXT:clubdirectory/Resources/Private/Language/FlexForms.xlf:category</label>
							<description>LLL:EXT:clubdirectory/Resources/Private/Language/FlexForms.xlf:category.description</description>
							<config>
								<type>select</type>
								<renderType>selectSingle</renderType>
								<foreign_table>sys_category</foreign_table>
								<foreign_table_where>AND parent=###PAGE_TSCONFIG_ID### ORDER BY title</foreign_table_where>
								<items type="array">
									<numIndex index="0">
										<numIndex index="0">LLL:EXT:clubdirectory/Resources/Private/Language/FlexForms.xlf:allCategories</numIndex>
										<numIndex index="1">0</numIndex>
									</numIndex>
								</items>
								<default>0</default>
								<size>1</size>
								<minitems>0</minitems>
							</config>
						</TCEforms>
					</settings.category>
					<settings.district>
						<TCEforms>
							<label>LLL:EXT:clubdirectory/Resources/Private/Language/FlexForms.xlf:district</label>
							<config>
								<type>select</type>
								<renderType>selectSingle</renderType>
								<foreign_table>tx_clubdirectory_domain_model_district</foreign_table>
								<foreign_table_where>AND tx_clubdirectory_domain_model_district.district != "" ORDER BY district</foreign_table_where>
								<items type="array">
									<numIndex index="0">
										<numIndex index="0">LLL:EXT:clubdirectory/Resources/Private/Language/FlexForms.xlf:allDistricts</numIndex>
										<numIndex index="1">0</numIndex>
									</numIndex>
								</items>
								<default>0</default>
								<size>1</size>
								<minitems>0</minitems>
							</config>
						</TCEforms>
					</settings.district>
				</el>
			</ROOT>
		</sDEFAULT>
	</sheets>
</T3DataStructure>
